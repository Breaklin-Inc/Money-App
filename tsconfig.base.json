{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "noUnusedLocals": false,
    "emitDecoratorMetadata": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "exclude": [
  "node_modules",
  "dist",
  ]
}
